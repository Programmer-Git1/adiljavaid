#include<stdio.h>
int main()
{
int num_1,num_2,num_3;
    printf("Enter three numbers\n");
    scanf("%d %d %d",&num_1,&num_2,&num_3);
    if(num_1>num_2 && num_2>num_3)
    printf("The largest number is %d %d %d\n",num_1,num_2,num_3);
    if(num_2>num_3 && num_3>num_1)
    printf("The largest number is %d %d %d\n",num_2,num_3,num_1);
    if(num_3>num_1&& num_1>num_2)
    printf("The largest number is %d d% %d\n",num_3,num_1,num_2);
    if(num_2>num_1 && num_1>num_3)
    printf("The largest number is %d %d %d\n",num_2,num_1,num_3);
    if(num_1>num_3 && num_3>num_2)
    printf("The largest number is %d %d %d\n",num_1,num_3,num_2);
    if(num_3>num_2 && num_2>num_1)
    printf("The largest number is %d %d %d\n",num_3,num_2,num_1);
}
